// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Apod {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now()) @db.date @unique
  updatedAt DateTime @updatedAt
  imageUrl  String
  title     String
  description String
  author    String
}

model Astrobin {
  id String @id @default(cuid())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  imageUrl String
  title String
  description String
  author String 
}